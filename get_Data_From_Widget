   var selectedCity = ZDK.Client.openPopup({
    api_name: 'city', // Replace with your widget's API name
    type: 'widget',
    header: 'My Custom Widget',
    animation_type: 4, // Example animation type
    height: '300px',
    width: '500px'
}, {
           data: "dataToPass" // Data passed to the widget
   
});


console.log("city ", selectedCity); // get data from widget 

